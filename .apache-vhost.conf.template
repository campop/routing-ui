<VirtualHost *:80>

	# Available URL(s)
	ServerName mobiledev.cyclestreets.net
	
	# Logging
	CustomLog /var/log/apache2/mobiledev-access.log combined
	ErrorLog /var/log/apache2/mobiledev-error.log

	# Where the files are
	DocumentRoot /var/www/mobiledev/
	
	# Allow access
	<Directory />
		Require all granted
	</Directory>
	
</VirtualHost>

